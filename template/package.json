{
  "name": "RNBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "format": "prettier --write --check .",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky install",
    "build:local": "cp ./.env.local ./.env && cd ios && pod install && react-native start",
    "build:development": "cp ./.env.development ./.env && cd ios && pod install && react-native start",
    "build:staging": "cp ./.env.staging ./.env && cd ios && pod install && react-native start",
    "build:production": "cp ./.env.production ./.env && cd ios && pod install && react-native start"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.1.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/devtools": "^6.0.20",
    "@react-navigation/native": "^6.1.9",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/ramda": "^0.29.9",
    "@typescript-eslint/parser": "^6.13.1",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "i18next": "^23.7.7",
    "lint-staged": "^15.1.0",
    "ramda": "^0.29.1",
    "react": "18.2.0",
    "react-i18next": "^13.5.0",
    "react-native": "0.72.7",
    "react-native-config": "^1.5.1",
    "react-native-flipper": "^0.212.0",
    "react-native-localize": "^3.0.4",
    "react-native-mmkv": "^2.11.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-sass-transformer": "^2.0.0",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^14.0.0",
    "react-redux": "^8.1.3",
    "redux-flipper": "^2.0.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.1.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "18.2.0",
    "sass": "^1.69.5",
    "typescript": "^5.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css, scss}": [
      "prettier --write --check .",
      "eslint --fix src/"
    ]
  },
  "engines": {
    "node": ">=20"
  }
}
